<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.codingfuture.mapper.ResetPasswordMapper">
    <update id="updatePwd">
        UPDATE emp SET pwd = #{newPwd} WHERE uuid = #{id}
    </update>
</mapper>